	<queryString>
		<![CDATA[select e.*,cla.descricao,REPLACE(FORMAT(sum(m.valor),2),',','') as Total

from movimentocartoes m
inner join cadcartoes c on m.cartao = c.codigo
inner join classificacaofinanceira cla on m.classificacao = cla.cod

INNER JOIN empresa e ON m.empresa_codempresa = e.codempresa

where m.pago = 0
and m.excluido=0

AND ($P{classificacao} IS NULL OR cla.cod = $P{classificacao})

AND ($P{dtemiini} IS NULL OR m.dataemissao >= $P{dtemiini})
AND ($P{dtemifim} IS NULL OR m.dataemissao <= $P{dtemifim})

AND ($P{dtvencini} IS NULL OR m.datavencimento >= $P{dtvencini})
AND ($P{dtvencfim} IS NULL OR m.datavencimento <= $P{dtvencfim})

group by cla.descricao,e.codempresa
order by cla.descricao;]]>
	</queryString>
